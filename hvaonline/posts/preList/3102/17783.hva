	$Color&#91;10&#93; = "orange"
	$Color&#91;11&#93; = "pink"
	$Color&#91;12&#93; = "silver"
	$Color&#91;13&#93; = "gray"
	$Color&#91;14&#93; = "maroon"
	$Color&#91;15&#93; = "green"
	$Color&#91;16&#93; = "teal"
	$Color&#91;17&#93; = "lime"
	$Array = StringSplit&#40;$Message, ""&#41;
	$Final = ""
	$NowColor = ""
	$iColorInt = 0
	Local $PreTags = ""
	Local $PreTags = ""
	If GUICtrlRead&#40;$BoldCheck&#41; = 1 Then
		$PreTags &= "&#91;b&#93;"
		$PosTags &= "&#91;/b&#93;"
	EndIf
	If GUICtrlRead&#40;$ItalicCheck&#41; = 1 Then
