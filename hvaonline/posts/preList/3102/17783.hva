		$PosTags &= "&#91;/i&#93;"
	EndIf
	If GUICtrlRead&#40;$UnderlinedCheck&#41; = 1 Then
		$PreTags &= "&#91;u&#93;"
		$PosTags &= "&#91;/u&#93;"
	EndIf
	For $i = 1 To $Array&#91;0&#93;
		If $Array&#91;$i&#93; &lt;&gt; " " And $Array&#91;$i&#93; &lt;&gt; @CR And $Array&#91;$i&#93; &lt;&gt; @LF Then
			$NowColor = $Color&#91;Mod&#40;$iColorInt, 18&#41;&#93;
			$Final &= "&#91;" & $NowColor & "&#93;" & $Array&#91;$i&#93;
			$iColorInt += 1
		Else
			$Final &= $Array&#91;$i&#93;
		EndIf
	Next
	$Final &= "&#91;/" & $NowColor & "&#93;"
	Return $PreTags & $Final & $PosTags
EndFunc
