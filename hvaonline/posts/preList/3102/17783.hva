Func ColorizeOrkut&#40;$Message&#41;
	Dim $Color&#91;18&#93;
	$Color&#91;0&#93; = "aqua"
	$Color&#91;1&#93; = "blue"
	$Color&#91;2&#93; = "navy"
	$Color&#91;3&#93; = "purple"
	$Color&#91;4&#93; = "violet"
	$Color&#91;5&#93; = "pink"
	$Color&#91;6&#93; = "yellow"
	$Color&#91;7&#93; = "gold"
