n2;<br/> char macn1[100];<br/> char tentep[100];<br/> printf("nhap vao ten FILE ban muon bo sung truoc:");<br/> gets(tentep);<br/> if((fp=fopen(tentep,"rb"))==NULL)<br/> {<br/> printf("khong tim duoc tep %s",tentep);getch();return;<br/> }<br/> fp1=fopen("TAM.DAT","wb");<br/> printf("\nnhap vao ma cong nhan ban muon bo sung truoc:");gets(macn1);<br/> while(fread(&cn1,sizeof(cn1),1,fp))<br/> if(strcmp(macn1,cn1.macn)!=0)fwrite(&cn1,sizeof(cn1),1,fp1);<br/> else<br/> {<br/> printf("\nnhap cong nhan can bo sung");<br/> printf("\nnhap ho va ten:");gets(cn2.ht);<br/> printf("\nnhap vao ma cong nhan:");gets(cn2.macn);<br/> printf("\nnhap he so luong:");scanf("%f",&cn2.hsl);<br/> printf("\nnhap so ngay cong:");scanf("%d",&cn2.snc);<br/> fwrite(&cn2,sizeof(cn2),1,fp1);<br/> fwrite(&cn1,sizeof(cn1),1,fp1);<br/> }<br/> remove(tentep);<br/> rename("TAM.DAT",tentep);<br/> fcloseall();<br/> printf("\nda bo sung xong.Moi xem file");<br/> }<br/> //-----------------------------------------------------------------------<br/> void bssau()<br/> {<br/> FILE *fp,*fp1;<br/> congnhan cn1,cn2;<br/> char macn1[100];<br/> char tentep[100];<br/> printf("nhap vao ten FILE ban muon bo sung sau:");<br/> gets(tentep);<br/> if((fp=fopen(tentep,"rb"))==NULL)<br/> {<br/> printf("khong tim duoc tep %s",tentep);getch();return;<br/> }<br/> fp1=fopen("TAM.DAT","wb");<br/> printf("\nnhap vao ma cong nhan ban muon bo sung sau:");gets(macn1);<br/> while(fread(&cn1,sizeof(cn1),1,fp))<br/> if(strcmp(macn1,cn1.macn)!=0)fwrite(&cn1,sizeof(cn1),1,fp1);<br/> else<br/> {<br/> fwrite(&cn1,sizeof(cn1),1,fp1);<br/> printf("\nnhap cong nhan can bo sung");<br/> printf("\nnhap ho va ten:");gets(cn2.ht);<br/> printf("\nnhap vao ma cong nhan:");gets(cn2.macn);<br/> printf("\nnhap he so luong:");scanf("%f",&cn2.hsl);<br/> printf("\nnhap so ngay cong:");scanf("%d",&cn2.snc);<br/> fwrite(&cn2,sizeof(cn2),1,fp1);<br/> }<br/> remove(tentep);<br/> rename("TAM.DAT",tentep);